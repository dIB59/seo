  "query": "\n            SELECT \n                p.id, p.url, p.title, p.status_code, p.load_time_ms,\n                COUNT(i.id) as issue_count\n            FROM pages p\n            LEFT JOIN issues i ON i.page_id = p.id\n            WHERE p.job_id = ?\n            GROUP BY p.id\n            ORDER BY issue_count DESC, p.url ASC\n            ",
